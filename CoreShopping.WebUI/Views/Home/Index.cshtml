@using CoreShopping.WebUI.Models
@model ProductListModel

<div class="row">
    <div class="col-md-3">
        <ul class="list-group">
            <li class="list-group-item">Sci-Fi</li>
            <li class="list-group-item">Horror</li>
            <li class="list-group-item">Commedy</li>
        </ul>
    </div>
    <div class="col-md-9">
        <partial name="_slider">
            @foreach (var item in Model.Products )
            {
                <p>@item.Name</p>
            }  
        </partial>
    </div>
</div>